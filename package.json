{
  "name": "graphql-rate-limit",
  "description": "Basic rate-limiting middleware for GraphQL. Use to limit repeated requests to queries and/or mutations.",
  "version": "0.1.0",
  "homepage": "https://github.com/ravangen/graphql-rate-limit#readme",
  "author": "Rob Van Gennip <ravangen@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ravangen/graphql-rate-limit.git"
  },
  "bugs": {
    "url": "https://github.com/ravangen/graphql-rate-limit/issues"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -d",
    "lint": "npm run tslint; npm run prettier",
    "tslint": "tslint --fix -c tslint.json 'src/**/*.ts'",
    "prettier": "prettier --trailing-comma all --single-quote --write 'src/**/*.ts'",
