    "rate-limit",
    "cost",
    "security"
  ],
  "dependencies": {
    "apollo-server": "^2.1.0",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.0"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.2",
    "prettier": "^1.14.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.1.3"
  }
